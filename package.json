{
  "name": "capraconsulting-nettsiden",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:ladle": "ladle build",
    "dev": "npm-run-all generate:css --parallel dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "ladle": "ladle serve",
    "dev-cloudflare": "cross-env NODE_ENV=development CLOUDFLARE_DEV=true npm-run-all generate:css build:remix --parallel dev:css dev-cloudflare:*",
    "dev-cloudflare:remix": "remix watch",
    "dev-cloudflare:wrangler": "wrangler pages dev ./public",
    "generate:css": "npx tailwindcss -o ./app/tailwind.css",
    "start": "cross-env NODE_ENV=production npm run dev-cloudflare:wrangler",
    "lint": "eslint \"./{app,studio}/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "sanity-codegen": "sanity-codegen"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "dependencies": {
    "@portabletext/react": "2.0.0",
    "@remix-run/cloudflare": "1.7.6",
    "@remix-run/react": "1.7.6",
    "@remix-run/server-runtime": "1.7.6",
    "@sanity/image-url": "^1.0.1",
    "cross-env": "7.0.3",
    "isbot": "3.6.5",
    "prism-react-renderer": "1.3.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sanity-codegen": "0.9.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.18.0",
    "@ladle/react": "2.4.5",
    "@remix-run/dev": "1.7.6",
    "@remix-run/eslint-config": "1.7.6",
    "@remix-run/serve": "1.7.6",
    "@tailwindcss/typography": "0.5.8",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.0",
    "prettier-plugin-tailwindcss": "0.1.13",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.3",
    "wrangler": "2.4.4"
  },
  "engines": {
    "node": ">=16"
  }
}
