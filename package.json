{
  "name": "capraconsulting-nettsiden",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:ladle": "ladle build",
    "dev": "npm-run-all generate:css --parallel \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "ladle": "ladle serve",
    "dev-cloudflare": "cross-env NODE_ENV=development CLOUDFLARE_DEV=true npm-run-all generate:css build:remix --parallel dev:css dev-cloudflare:*",
    "dev-cloudflare:remix": "remix watch",
    "dev-cloudflare:wrangler": "wrangler pages dev ./public",
    "generate:css": "npx tailwindcss -o ./app/tailwind.css",
    "start": "cross-env NODE_ENV=production npm run dev-cloudflare:wrangler",
    "lint": "eslint \"./{app,studio}/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "npm run lint -- --fix"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "dependencies": {
    "@fontsource/source-sans-pro": "4.5.11",
    "@portabletext/react": "2.0.0",
    "@remix-run/cloudflare": "1.11.1",
    "@remix-run/react": "1.11.1",
    "@remix-run/server-runtime": "1.11.1",
    "@sanity/code-input": "3.0.1",
    "@sanity/dashboard": "3.1.2",
    "@sanity/image-url": "1.0.2",
    "@sanity/vision": "3.2.3",
    "cross-env": "7.0.3",
    "isbot": "3.6.5",
    "prism-react-renderer": "1.3.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.7.1",
    "react-youtube": "10.1.0",
    "remix-utils": "6.0.0",
    "sanity": "3.2.3",
    "sanity-plugin-dashboard-widget-document-list": "1.0.1",
    "sanity-typed-schema-builder": "2.1.0",
    "styled-components": "5.3.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.19.0",
    "@ladle/react": "2.4.5",
    "@remix-run/dev": "1.11.1",
    "@remix-run/eslint-config": "1.11.1",
    "@remix-run/serve": "1.11.1",
    "@tailwindcss/typography": "0.5.9",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/styled-components": "5.1.26",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "9.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "prettier-plugin-tailwindcss": "0.2.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4",
    "wrangler": "2.8.0"
  },
  "engines": {
    "node": ">=16"
  }
}
